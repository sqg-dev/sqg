  "main": "index.js",
  "type": "module",
  "bin": {
    "sqg": "./sqg",
    "sqg-dist": "./dist"
  },
  "scripts": {
    "build": "tsc --noEmit && tsdown src/sqg.ts && cp -r src/templates dist",
