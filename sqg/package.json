    "dotenv": "catalog:",
    "es-toolkit": "catalog:",
    "handlebars": "catalog:",
    "pg": "catalog:",
    "pg-types": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-java": "catalog:",
    "yaml": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/better-sqlite3": "catalog:",
