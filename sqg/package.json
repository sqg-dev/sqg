    "@biomejs/biome": "2.3.3",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.5",
    "@vitest/ui": "^3.2.4",
    "tsdown": "0.17.0-beta.5",
    "typescript": "^5.8.3",
    "tsx": "^4.20.3",
    "vitest": "^3.2.4"
  }
}
