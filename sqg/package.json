{
  "name": "sqg",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "bin": {
    "sqg": "./sqg",
    "sqg-dist": "./dist"
  },
  "scripts": {
    "build": "tsc --noEmit && tsdown src/sqg.ts && cp -r src/templates dist",
    "lezer-gen": "lezer-generator src/parser/sql.grammar -o src/parser/sql-parser.ts",
    "test-grammar": "tsx src/test-grammar.ts",
    "check": "biome check --write src/",
    "check-errors": "pnpm biome check --write --diagnostic-level=error src/",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "keywords": [],
  "author": "Uwe Maurer",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@duckdb/node-api": "catalog:",
    "@duckdb/node-bindings-linux-x64": "catalog:",
    "@lezer-unofficial/printer": "^1.0.1",
    "@lezer/common": "^1.2.3",
    "@lezer/generator": "^1.8.0",
    "@lezer/lr": "^1.4.2",
    "better-sqlite3": "^12.2.0",
    "commander": "^14.0.0",
    "consola": "^3.4.2",
    "dotenv": "^17.0.1",
    "es-toolkit": "^1.39.6",
    "handlebars": "^4.7.8",
    "pg": "^8.16.3",
    "pg-types": "^4.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-java": "^2.7.1",
    "yaml": "^2.8.0",
    "zod": "^4.0.5"
  },
