    "check": "biome check --write src/",
    "check:errors": "pnpm biome check --write --diagnostic-level=error src/",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
